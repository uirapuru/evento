{
    "name": "dende/evento",
    "authors": [
        {
            "name": "Grzegorz Kaszuba",
            "email": "uirapuruadg@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",

        "friendsofsymfony/user-bundle": "~2.0@dev",

        "doctrine/common": "2.6.1",
        "doctrine/data-fixtures": "1.1.1",
        "doctrine/dbal": "2.5.4",
        "doctrine/doctrine-fixtures-bundle": "2.3.0",
        "doctrine/orm": "2.5.4",


        "nette/bootstrap": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/swiftmailer-bundle": "^2.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/generator-bundle": "^3.0",
        "league/tactician-bundle": "^0.4.0",
        "sensio/framework-extra-bundle": "^3.0",
        "dende/calendar-bundle": "dev-master",
        "knplabs/doctrine-behaviors": "~1.1"
    },
    "require-dev": {
        "behat/behat": "^3.0",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.1",
        "behat/mink-zombie-driver": "~1.2",
        "behat/mink-selenium2-driver": "~1.2",
        "behat/mink-browserkit-driver": "^1.3",

        "phpmd/phpmd": "^2.3",
        "mayflower/php-codebrowser": "1.1.1",
        "sebastian/phpcpd": "2.0.*@dev",
        "pdepend/pdepend": "2.0.*",

        "phing/phing": "^2.13",
        "phpunit/phpunit": "^5.1",
        "squizlabs/php_codesniffer": "^2.5",
        "phpspec/phpspec": "^2.4",
        "mockery/mockery" : "*",

        "liip/functional-test-bundle": "^1.3",
        "hautelook/alice-bundle": "^1.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "autoload": {
        "psr-0": {
            "": "src/",
            "Dende\\CalendarBundle": "vendor/dende/calendar-bundle/"
        },
        "psr-4": {
            "": "src/",
            "SymfonyStandard\\": "app/",
            "Dende\\CalendarBundle\\": "vendor/dende/calendar-bundle/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    }
}
